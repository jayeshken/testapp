<h1>Listing records</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @records.each do |record| %>
      <tr>
        <td><%= record.name %></td>
        <td><%= record.subject %></td>
        <td><%= record.IT1 %></td>
        <td><%= record.IT2 %></td>
        <td><%= record.subject %></td>
        <td><%= record.IT1 %></td>
        <td><%= record.IT2 %></td>
        <td><%= record.subject %></td>
        <td><%= record.IT1 %></td>
        <td><%= record.IT2 %></td>
        <td><%= record.subject %></td>
        <td><%= record.IT1 %></td>
        <td><%= record.IT2 %></td>
        <td><%= record.subject %></td>
        <td><%= record.IT1 %></td>
        <td><%= record.IT2 %></td>
        <td><%= record.subject %></td>
        <td><%= record.IT1 %></td>
        <td><%= record.IT2 %></td>
        <td><%= link_to 'Show', record %></td>
        <td><%= link_to 'Edit', edit_record_path(record) %></td>
        <td><%= link_to 'Destroy', record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

