<h1>Listing items</h1>

<table>
  <thead>
    <tr>
      <th>Faculty</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.faculty %></td>
        <td><%= item.subject %></td>
        <td><%= item.IT1 %></td>
        <td><%= item.IT2 %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_item_path %>
