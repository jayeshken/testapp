<h1>Listing cats</h1>

<table>
  <thead>
    <tr>
      <th>Faculty</th>
      <th>Subject</th>
      <th>It1</th>
      <th>It2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cats.each do |cat| %>
      <tr>
        <td><%= cat.faculty %></td>
        <td><%= cat.subject %></td>
        <td><%= cat.IT1 %></td>
        <td><%= cat.IT2 %></td>
        <td><%= link_to 'Show', cat %></td>
        <td><%= link_to 'Edit', edit_cat_path(cat) %></td>
        <td><%= link_to 'Destroy', cat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cat', new_cat_path %>
